map 3DAB_pocket.A.map                        # atom-specific affinity map
map 3DAB_pocket.C.map                        # atom-specific affinity map
map 3DAB_pocket.F.map                        # atom-specific affinity map
map 3DAB_pocket.I.map                        # atom-specific affinity map
map 3DAB_pocket.NA.map                       # atom-specific affinity map
map 3DAB_pocket.Cl.map                       # atom-specific affinity map
map 3DAB_pocket.OA.map                       # atom-specific affinity map
map 3DAB_pocket.N.map                        # atom-specific affinity map
map 3DAB_pocket.S.map                        # atom-specific affinity map
map 3DAB_pocket.Br.map                       # atom-specific affinity map
map 3DAB_pocket.SA.map                       # atom-specific affinity map
map 3DAB_pocket.HD.map                       # atom-specific affinity map
map 3DAB_pocket.H.map                        # atom-specific affinity map
elecmap 3DAB_pocket.e.map                    # electrostatic potential map
dsolvmap 3DAB_pocket.d.map                   # desolvation potential map
dielectric -0.1465                           # <0, AD4 distance-dep.diel;>0, constant

